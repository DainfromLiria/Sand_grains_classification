{
    "mode": "train",
    "model": "DeepLabV3Plus",
    "encoder": "resnet50",
    "encoder_weights": "imagenet",
    "batch_size": 8,
    "lr": 0.0001,
    "epoch_count": 300,
    "loss_function": "FocalTverskyLoss",
    "optimizer": "AdamW",
    "patience": 100,
    "use_patches": false,
    "patch_size": 512,
    "overlap_rate": 0.5,
    "patch_stride": 256,
    "use_augmentations": true,
    "use_preprocessing": false,
    "use_ca": true,
    "ca_t0": 10,
    "ca_tmult": 2,
    "classes_count": 5,
    "use_clipping": false,
    "use_resize": false,
    "scheduler": "CosineAnnealingLR"
}